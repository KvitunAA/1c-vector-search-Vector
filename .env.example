# Все параметры ОБЕЗЛИЧЕНЫ. Укажите реальные значения для своего окружения.

# Доступ к векторной БД
VECTOR_DB_URL=http://your-vector-db-host:9000
VECTOR_DB_USER=your_vector_db_user
VECTOR_DB_PASSWORD=your_vector_db_password
VECTOR_DB_API_KEY=your_vector_db_api_key

# Настройки модели эмбеддингов (название/идентификатор модели не привязан к конкретному провайдеру)
EMBEDDING_MODEL_NAME=your-embedding-model-name

# Режим транспорта MCP (false - streamable-http, true - SSE)
MCP_USE_SSE=false

